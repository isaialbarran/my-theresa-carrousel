{
  "version": 2,
  "builds": [
    {
      "src": "server/index.js",
      "use": "@vercel/node@3.0.7"
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/dist/client/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/server/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "VITE_TMDB_BASE_URL": "https://api.themoviedb.org/3"
  }
}